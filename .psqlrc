\set VERBOSITY verbose
\setenv PSQL_EDITOR "vim"
\set PROMPT1 '%n@%m %/ %R%x%# '
\set PROMPT2 '             '
\set HISTCONTROL ignoredups
\pset null <null>
\pset linestyle unicode
\pset unicode_border_linestyle double
\pset unicode_header_linestyle double
\pset unicode_column_linestyle single
\set gxweb_version 'SELECT * FROM gxweb.schema ORDER BY version DESC LIMIT 1;'
\setenv PAGER 'pspg -s 5 --no-mouse'
\set less '\\setenv PAGER \'less -S\''
